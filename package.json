{
  "name": "@webantic/dependency-mapper",
  "version": "2.1.0",
  "description": "Recursively extract Meteor dependencies declared in npm modules' package.json files",
  "types": "lib/index.d.ts",
  "main": "lib/index",
  "scripts": {
    "dist": "npm run format:ts && npm run build",
    "prepublish": "npm run dist",
    "format:ts": "tslint --fix --force --format stylish --config tslint.json --project tsconfig.json",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/webantic/dependency-mapper.git"
  },
  "author": "Webantic (Carl Evans)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webantic/dependency-mapper/issues"
  },
  "homepage": "https://github.com/webantic/dependency-mapper#readme",
  "dependencies": {
    "debug": "^3.0.1"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/node": "^8.10.26",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^6.0.1",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.9.2"
  },
  "peerDependencies": {
    "@types/debug": "0.0.30",
    "@types/node": "^8.10.26"
  }
}
